<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="~/Scripts/easyui/themes/icon.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.11.0.js"></script>
    <link href="~/Scripts/easyui/themes/default/easyui.css" rel="stylesheet" />
    <script src="~/Scripts/easyui/jquery.easyui.js"></script>
    <script src="~/Scripts/easyui/locale/easyui-lang-zh_CN.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.validate.js"></script>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="robots" content="all" />
    <meta name="Copyright" content="中国银联" />
    <meta content="银联在线支付是中国银联联合商业银行共同推出的集成化、综合性、开放性网上支付平台,全面支持各类型银联卡,涵盖认证支付、快捷支付、普通支付、网银支付等多种支付方式,可为银联卡持卡人的购物缴费、商旅预定、慈善捐款、转账还款等提供安全、快捷、多选择、全球化的支付服务." name="description">
    <meta content="银联,银联在线,中国银联,银联支付,银联在线支付,中国银联在线,银联在线商城,银联在线还款,中国银联在线支付,银联支付平台" name="keywords">
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="-1">

    <link href="~/omer/resources/zh_cn/css/global.css" rel="stylesheet" type="text/css" />
    <link href="~/omer/resources/zh_cn/css/main.css" rel="stylesheet" type="text/css" />
    <link href="~/omer/resources/zh_cn/css/redmond/jquery-ui-1.9.2.custom.min.css" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="~/omer/resources/js/jquery/jquery-1.7.2.min.js"></script>
    <script type="text/javascript" src="~/omer/resources/js/jquery/jquery-ui-1.9.2.custom.min.js"></script>
    <script type="text/javascript" src="~/omer/resources/js/My97DatePicker/WdatePicker.js"></script>
    
    <script src="~/omer/resources/js/upms.js"></script>
    <script src="~/omer/resources/js/upms-lang-zh-cn.js"></script>
    @RenderSection("Head", required: false)
</head>
<body class="body-bg">
    <input type="hidden" id="sessionUUID" name="sessionUUID" value=@ViewData["sessionUUID"]>
    @*<form runat="server">*@
        <div class="body-bg-left">
            <div class="body-bg-right">
                <!--S header -->
                <div id="header">
                    <div class="header-logo"> <a href='@Url.Action("Index", "Home")'>首页</a> </div>
                    <div class="header-user">
                        <ul>
                            <li>欢迎, </li>
                            <!--<li><a href="javascript: void(0);" class="login-out" id="editPwdFuncion">修改密码</a></li>-->
                            <li><a href='@Url.Action("logOut", "Account")' class="login-out">安全退出</a></li>
                            <!--<li><a target="_blank" href="~/static/help/default.html"> 帮助</a></li>-->
                        </ul>
                        <div class="clear"></div>
                    </div>
                </div>
                <!--E header-->
                <div class="nav">
                    <div class="nav_left">当前位置：<span id="currAddr_main">首页</span> &gt; <b><span id="currAddr_child">欢迎页</span></b></div>
                    <!--<div class="nav_right">
                        <div class="note_t">公告：</div>
                        <div class="note_c">
                            <iframe src="https://online.unionpay.com/static/notice/notice_iframe_148.html" scrolling="no" allowtransparency="true" height="20px" width="95%" frameborder="0"></iframe>
                        </div>
                        <div class="note_m"><a href="http://online.unionpay.com/static/notice/notice_list_148.html" target="_blank">更多</a></div>
                    </div>-->
