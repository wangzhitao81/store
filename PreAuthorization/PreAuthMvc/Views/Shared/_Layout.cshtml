                            <em class="menu_open"></em>
                        </div>
                        <ul>
                            <li>
                                <a href='javascript:void(0);' action='@Url.Action("Index", "Agency")'>机构管理</a>
                                <em></em>
                            </li>
                            <li>
                                <a href='javascript:void(0);' action='@Url.Action("Index", "Function")'>功能管理</a>
