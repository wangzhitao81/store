                            <div class="top_m_l"></div>
                            <div class="top_m_r"></div>
                        </div>
                        <div class="p_menu">
                            <span class="icon_web"></span>
                            <em class="title">业务处理</em>
                            <em class="menu_open menu_close"></em>
                        </div>
                        <ul>
                            <li>
                                <a href='javascript:void(0);' action='@Url.Action("Index","PreAuth")' >
                                    预授权
                                </a>
                                <em></em>
                            </li>
                            <li>
                                <a href='javascript:void(0);' action='@Url.Action("Index", "CancelPreAuth")'>
                                    撤销预授权
                                </a>
                                <em></em>
                            </li>
                            <li>
                                <a href='javascript:void(0);' action='@Url.Action("CompleteIndex", "PreAuth")'>
                                    完成预授权
                                </a>
                                <em></em>
                            </li>
                            <li>
                                <a href='javascript:void(0);' action='@Url.Action("CancelComplete", "PreAuth")'>
                                    完成后撤销
                                </a>
                                <em></em>
                            </li>
                            <li>
                                <a href='javascript:void(0);' action='@Url.Action("Index", "QueryPreAuth")'>
                                    预授权客户查询
                                </a>
                                <em></em>
                            </li>
                        </ul>
                        <div class="p_menu">
                            <span class="icon_user"></span>
