<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="PreAuthEntityLib.Vo" assembly="PreAuthEntityLib">

  <class name="UserVo" table="Sys_User" >

    <!-- Primary Key(s) -->
    <id name="Id" column="RecordId">
      <!--<generator class="identity" />-->
      <generator class="sequence">
        <param name="sequence">TestSeq</param>
      </generator>
    </id>

    <property name="UserCode" column="UserCode" not-null="true" />
    <property name="UserName" column="UserName" not-null="true" />
    <property name="RoleCode" column="RoleCode" not-null="true" />
    <property name="Password" column="Password" not-null="true" />
    <property name="AgencyCode" column="AgencyCode" not-null="true" />
  </class>
</hibernate-mapping>