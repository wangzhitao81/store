                    <!--end of left_menu-->
                    <div id="_mainContent_" class="main_content">
                        <div class="right_content">
                            @RenderBody()
                        </div><!--end of right_content-->
                    </div>
                    <!--end of main_content-->
                    <div class="clear"></div>
                </div>
            </div>
        </div>
        <!--S footer -->
        <div id="footer">
            <p class="fl">
                <!--<a href="http://www.unionpay.com/" target="_blank">UnionPay Web</a>|
                <a href="http://online.unionpay.com/" target="_blank">Online Payment</a>|
                <a href="https://online.unionpay.com/static/common/about.html">About Us</a>|
                <a href="~/omer/DOC/docMng!queryDoc.action" target="_blank">Document Download</a>-->
            </p>
            <p class="fr"> Copyright © 2002 – 2014 All rights reserved. </p>
        </div>
        <!--E footer-->
    @*</form>*@
</body>

</html>
<script type="text/javascript">
    upms.installWebObj($("#_mainContent_"));
    upms.WEBMAIN.initLayout();
    upms.WEBMAIN.initMenu("PreAuth/Index");
    //upms.WEBLANG.initLang($(".language"));
    upms.installOverLay({// 安装ajax遮罩
        type: "font",
        opacity: 0.8,
        fontcss: {
            color: "black"
        }
    });
</script>
